{
  "compilerOptions": {
    "target": "es6", 
    "module": "commonjs", 
    "strict": true, 
    "esModuleInterop": true,
    "skipLibCheck": true, 
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node", 
    "resolveJsonModule": true, 
    "noEmit": true, 
    "allowJs": true 
  },
  "include": [
    "**/*.ts", 
    "**/*.js" 
, "playwright.config.ss"  ]
}
